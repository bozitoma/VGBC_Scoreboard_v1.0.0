{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "player1": {
            "type": "object",
            "properties": {
                "name": {"type": "string"},
                "prefix": {"type": "string"},
                "xID": {"type": "string"},
                "score": {"type": "number"}
            },
            "required": ["name", "prefix", "score"]
        },
        "player2": {
            "type": "object",
            "properties": {
                "name": {"type": "string"},
                "prefix": {"type": "string"},
                "xID": {"type": "string"},
                "score": {"type": "number"}
            },
            "required": ["name", "prefix", "score"]
        },
        "fullRoundText": {
            "type": "string"
        }
    },
    "required": ["player1", "player2", "fullRoundText"]
}
